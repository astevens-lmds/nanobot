# nanobot environment variables
# Copy to .env and fill in your values

# OpenRouter API key (recommended - access to all models)
OPENROUTER_API_KEY=sk-or-v1-xxx

# Or use a direct provider:
# ANTHROPIC_API_KEY=sk-ant-xxx
# OPENAI_API_KEY=sk-xxx
# DEEPSEEK_API_KEY=xxx

# Brave Search API key (optional, for web search tool)
BRAVE_SEARCH_API_KEY=BSA-xxx

# Telegram bot token (optional, for Telegram channel)
TELEGRAM_BOT_TOKEN=123456:ABC-xxx

# Telegram user IDs allowed to use the bot (comma-separated)
TELEGRAM_ALLOW_FROM=123456789

# Groq API key (optional, for voice transcription)
GROQ_API_KEY=gsk_xxx
